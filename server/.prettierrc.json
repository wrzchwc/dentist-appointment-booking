{
  "trailingComma": "es5",
  "tabWidth": 4,
  "singleQuote": true,
  "semi": true,
  "printWidth": 120,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "html"
      }
    },
    {
      "files": "*.component.html",
      "options": {
        "parser": "angular"
      }
    }
  ]
}