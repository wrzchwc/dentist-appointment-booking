{
  "name": "dentist-appointment-booking-server",
  "version": "1.0.0",
  "description": "Dentist appointment booking API, BSc Thesis by Jakub Wierzchowiec 2022",
  "main": "dist/src/server.js",
  "scripts": {
    "lint": "npx eslint src jest.config.ts",
    "lint:fix": "npm run lint --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test": "jest --config jest.config.ts",
    "build": "npm run format && tsc",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/src/server.js\"",
    "start": "npm run build && node dist/src/server.js",
    "load:services": "npm run build && node dist/scripts/data/services.js",
    "load:appointments": "npm run build && node dist/scripts/data/appointments.js"
  },
  "author": "Jakub Wierzchowiec",
  "license": "ISC",
  "dependencies": {
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.0",
    "passport": "^0.5.3",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.8.0",
    "sequelize": "^6.24.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.8.3",
    "@types/passport": "^1.0.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.1.2",
    "keygrip": "^1.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
